class CDE{
    int a;
    int b;
    public int foo(){
        a = 10;
        println(a);
        return 0;
    }
}
class ABC{

    public static void main(String[] args)
    {{
        CDE c1;
        int b;
        c1 = new CDE();
        b = c1.a;
        println(b);
        c1.foo();
    }}

}
